{
  "displayName": "GioPic",
  "name": "giopic",
  "version": "2.1.0",
  "description": "GioPic Web Extension,图片上传扩展程序提供lsky,EasyImages,chevereto,阿里云oss,AWS S3,等程序的上传",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm-run-all clear prepare --parallel dev:*",
    "dev:main": "vite",
    "dev:bg": "vite build --config vite.config.bg.ts --watch",
    "dev:content": "vite build --config vite.config.content.ts --watch",
    "dev:page": "vite build --config vite.config.page.ts --watch",
    "build": "cross-env NODE_ENV=production npm-run-all clear prepare --parallel build:*",
    "build:web": "vite build",
    "build:ext": "vite build --config vite.config.bg.ts && vite build --config vite.config.content.ts && vite build --config vite.config.page.ts",
    "build:check": "vue-tsc --build",
    "preview": "vite preview",
    "prepare": "esno manifest/prepare.ts",
    "test": " esno test/test.ts",
    "clear": "rimraf --glob dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.966.0",
    "@aws-sdk/lib-storage": "^3.966.0",
    "@iconify-json/carbon": "^1.2.15",
    "@types/fs-extra": "^11.0.4",
    "ali-oss": "^6.23.0",
    "axios": "^1.13.2",
    "cos-js-sdk-v5": "^1.10.1",
    "idb": "^8.0.3",
    "naive-ui": "^2.43.2",
    "pinia": "^3.0.4",
    "vfonts": "^0.0.3",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.3",
    "@types/ali-oss": "^6.23.0",
    "@types/node": "^24.10.4",
    "@types/webextension-polyfill": "^0.12.4",
    "@unocss/preset-icons": "^66.5.12",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "chokidar": "^4.0.3",
    "cross-env": "^7.0.3",
    "esno": "^4.8.0",
    "fs-extra": "^11.3.3",
    "npm-run-all2": "^8.0.4",
    "rimraf": "^6.1.2",
    "typescript": "~5.9.3",
    "unocss": "^66.5.12",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.3.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.2"
  }
}
